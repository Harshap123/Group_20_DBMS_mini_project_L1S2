CREATE TABLE user
    -> (
    -> User_id VARCHAR(10),
    -> Password VARCHAR(20),
    -> Username VARCHAR(20),
    -> Fname VARCHAR(50),
    -> Lname VARCHAR(50),
    -> Dob DATE,
    -> Tele_no CHAR(12),
    -> Email VARCHAR(50),
    -> PRIMARY KEY(User_id)
    -> );

