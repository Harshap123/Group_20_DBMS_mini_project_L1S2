CREATE TABLE student
(
Stu_id VARCHAR(10),
Reg_no CHAR(12) NOT NULL,
Status ENUM('Proper','Repeat','Suspend') DEFAULT 'Proper',
Batch INT,
Dep_id VARCHAR(10) NOT NULL,
PRIMARY KEY(Stu_id),
FOREIGN KEY (Dep_id) REFERENCES department (Dep_id)
ON UPDATE CASCADE
ON DELETE CASCADE
); 
