//student_course

CREATE TABLE student_course
(
Course_id VARCHAR(10),
Stu_id VARCHAR(10),
PRIMARY KEY(Course_id,Stu_id),
FOREIGN KEY (Course_id) REFERENCES course (Course_id)
ON UPDATE CASCADE
ON DELETE CASCADE,
FOREIGN KEY (Stu_id) REFERENCES student (Stu_id)
ON UPDATE CASCADE
ON DELETE CASCADE
);

