//course table

CREATE TABLE course
(
    Course_id VARCHAR(10),
    Type ENUM('Theory','Practical','Both') DEFAULT 'Theory',
    C_code VARCHAR(10),
    Credit INT,
    C_name VARCHAR(50),
    Lec_id VARCHAR(10),
    Dep_id VARCHAR(10),
    PRIMARY KEY(Course_id),
    FOREIGN KEY (Lec_id) REFERENCES lecturer (Lec_id)
        ON UPDATE CASCADE
        ON DELETE CASCADE,
    FOREIGN KEY (Dep_id) REFERENCES department (Dep_id)
        ON UPDATE CASCADE
        ON DELETEÂ CASCADE
);

INSERT INTO course 
(Course_id, Type, C_code, Credit, C_name, Lec_id, Dep_id) 
VALUES
