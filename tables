//session table

CREATE TABLE session
(
Session_id VARCHAR(10),
Session_num INT DEFAULT 0,
Date DATE,
Duration TIME,
Course_id VARCHAR(10),
PRIMARY KEY(Session_id),
FOREIGN KEY (Course_id) REFERENCES course (Course_id)
ON UPDATE CASCADE
ON DELETE CASCADE
);


